{
  "name": "lion",
  "version": "3.0.0",
  "description": "A better Discord bot for the UCF Computer Science Discord",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint . --fix --cache --ext .js,.ts",
    "lint:check": "eslint . --cache --ext .js,.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "commands:register": "npm run build && node dist/deploy-commands.js register",
    "commands:delete": "npm run build && node dist/deploy-commands.js delete",
    "prepare": "husky install"
  },
  "author": "Brandon Rubio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluffdev/lion3"
  },
  "bugs": {
    "url": "https://github.com/bluffdev/lion3/issues"
  },
  "keywords": [
    "Discord",
    "UCF",
    "University of Central Florida",
    "CS",
    "Computer Science"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "mongoose": "^7.4.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.4",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
